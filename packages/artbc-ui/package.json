{
  "name": "ui-artbc",
  "version": "0.0.0",
  "description": "Add components to your apps.",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "name": "artbycode-id",
    "url": "https://twitter.com/artbycode-id"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artbycode-id/artbc.git",
    "directory": "packages/artbc-ui"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "components",
    "ui",
    "tailwind",
    "radix-ui",
    "artbc-ui"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist && rimraf components",
    "start:dev": "cross-env COMPONENTS_REGISTRY_URL=http://localhost:3001 node dist/index.js",
    "start": "node dist/index.js",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "release": "changeset version",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm build && pnpm publish --access public",
    "test": "vitest run"
  },
  "devDependencies": {
    "@artbc/typescript-config": "workspace:*",
    "@types/babel__core": "^7.20.5",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash.template": "^4.5.3",
    "@types/prompts": "^2.4.9",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "type-fest": "^4.18.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "@babel/core": "^7.24.5",
    "@babel/parser": "^7.24.5",
    "@babel/plugin-transform-typescript": "^7.24.5",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cosmiconfig": "^9.0.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "https-proxy-agent": "^7.0.4",
    "lodash.template": "^4.5.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "recast": "^0.23.6",
    "ts-morph": "^22.0.0",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.23.7"
  }
}
